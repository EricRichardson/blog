<h1>Sign In</h1>

<%= form_tag sessions_path do %>
<div>
  <%= label_tag :email %>
  <%= text_field_tag :email %>
</div>
<div>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>
<%= submit_tag "Sign In" %>
<%= link_to "Forgot password?", new_password_reset_path %>
<% end %>

<%= link_to "Sign in with Twitter", sign_in_with_twitter_path %>
